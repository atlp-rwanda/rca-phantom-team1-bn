{
  "name": "swagger-api-library",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=staging node src/index.js",
    "prod": "cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "test": "cross-env NODE_ENV=staging && nyc mocha",
    "lint": "eslint src",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:seeds": "sequelize-cli db:seed:all",
    "db:reseed": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.6",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "nodemon": "^2.0.21",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.1",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^13.13.15",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.8.1",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.20",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.9.5"
  }
}
