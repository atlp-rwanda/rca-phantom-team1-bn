{
  "name": "swagger-api-library",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=staging babel-node src/index.js",
    "prod": "cross-env NODE_ENV=production babel-node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "test": "cross-env NODE_ENV=staging nyc mocha --recursive ./src/test",
    "lint": "npm run format && eslint ./src --ext .js --fix",
    "format": "prettier --config .prettierrc 'src/**' --write",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:seeds": "sequelize-cli db:seed:all",
    "db:reseed": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "db:create:admin": "node src/db/seeds/create-admin.js",
    "db:undo":"sequelize-cli db:migrate:undo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.6",
    "typescript": "^3.9.7",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "nodemon": "^2.0.21",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.1",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/sinon": "^10.0.13",
    "@types/express": "^4.17.6",
    "@types/mocha": "^8.0.3",
    "@types/node": "^13.13.15",
    "chai": "^4.3.7",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.20",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.9.5",
    "prettier": "^2.8.4",
    "sinon": "^15.0.2",
    "assert": "^2.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2"
  },
  "nyc": {
    "branches": "&gt;80",
    "lines": "&gt;80",
    "functions": "&gt;80",
    "statements": "&gt;80",
    "check-coverage": true,
    "reporter": [
      "text-summary",
      "lcov"
    ]
  },
  "engines": {
    "node": ">=16"
  }
}
