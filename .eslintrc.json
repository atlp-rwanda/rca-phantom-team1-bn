{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "2022"
    },
    "plugins": [
        "prettier",
        "@babel"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "consistent-return": "off"
    },
    "env": {
        "node": true,
        "mocha": true,
        "es2021": true
    }
}